{
  "name": "mern_app",
  "version": "1.0.0",
  "description": "mern steck beck-end",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "listen": "nodemon app.js",
    "client": "npm run start --prefix client_side",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run listen\" \"npm run client\""
  },
